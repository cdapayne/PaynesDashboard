# PaynesDashboard Environment Configuration
# Copy this file to .env and fill in your credentials

# ============================================
# Phase 2: Analytics Integrations
# ============================================

# Apple App Store Connect
APPLE_ISSUER_ID=your-issuer-id
APPLE_KEY_ID=your-key-id
APPLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----
your-private-key-content
-----END PRIVATE KEY-----"
APPLE_VENDOR_NUMBER=your-vendor-number

# Google Play Developer Console
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret
GOOGLE_PACKAGE_NAME=com.yourcompany.yourapp

# IngramSpark
INGRAMSPARK_API_KEY=your-api-key
INGRAMSPARK_PUBLISHER_ID=your-publisher-id

# Draft2Digital
D2D_API_KEY=your-api-key
D2D_ACCOUNT_ID=your-account-id

# ============================================
# Phase 3: Social & RSS Widgets
# ============================================

# Facebook
FACEBOOK_CLIENT_ID=your-facebook-app-id
FACEBOOK_CLIENT_SECRET=your-facebook-app-secret
FACEBOOK_PAGE_ID=your-page-id

# YouTube
YOUTUBE_CLIENT_ID=your-google-client-id
YOUTUBE_CLIENT_SECRET=your-google-client-secret
YOUTUBE_CHANNEL_ID=your-channel-id

# TikTok
TIKTOK_CLIENT_KEY=your-client-key
TIKTOK_CLIENT_SECRET=your-client-secret

# Amazon Affiliate
AMAZON_ACCESS_KEY_ID=your-access-key-id
AMAZON_SECRET_ACCESS_KEY=your-secret-access-key
AMAZON_PARTNER_TAG=your-partner-tag
AMAZON_MARKETPLACE=www.amazon.com

# ============================================
# Phase 4: Social Media Campaign Management
# ============================================

# OnlySocial
ONLYSOCIAL_API_KEY=your-api-key
ONLYSOCIAL_ACCOUNT_ID=your-account-id

# Postly
POSTLY_API_KEY=your-api-key
POSTLY_WORKSPACE_ID=your-workspace-id

# ============================================
# Phase 5: AI Content Generation
# ============================================

# OpenAI
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORG_ID=your-organization-id
